---
import PageHeader from '../components/PageHeader.astro';
const { title } = Astro.props;
---

<html lang='en'>
  <head>
    <meta charset='utf-8' />
    <meta name='viewport' content='width=device-width' />
    <meta name='generator' content={Astro.generator} />
    <title>{title}</title>
  </head>
  <body>
    <PageHeader />

    <slot />
  </body>
</html>
